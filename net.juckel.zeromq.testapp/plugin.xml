            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="net.juckel.zeromq.testapp.Subscriber">
         </run>
      </application>
   </extension>
   <extension
         id="publisher"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
